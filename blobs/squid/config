include /etc/squid/acl/*.acl

cache deny all
http_access deny all

httpd_suppress_version_string on
follow_x_forwarded_for allow localhost
follow_x_forwarded_for deny all
forwarded_for delete
request_header_access X-Forwarded-For deny all
reply_header_access X-Cache deny all
reply_header_access X-Cache-Lookup deny all

http_port 10.85.20.10:3128 connection-auth=off

buffered_logs on
strip_query_terms on
logformat rfc5424 %master_xaction [request@99999 act="%Ss" ct="%mt" rc="%>Hs" rs="%<st" rt="%tr" ua="%{User-Agent}>h"] %>a:%>p %rm %ru
pid_filename /run/squid.pid
cache_effective_user squid squid
cache_log stdio:/var/log/squid/cache.log
access_log stdio:/var/log/squid/access.log logformat=rfc5424
access_log syslog:local2.info logformat=rfc5424
